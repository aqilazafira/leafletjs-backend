{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/locations",
            "dest": "index.js",
            "methods": [
                "GET",
                "POST",
                "DELETE",
                "OPTIONS"
            ]
        },
        {
            "src": "/api/locations/(.*)",
            "dest": "index.js",
            "methods": [
                "GET",
                "POST",
                "DELETE",
                "OPTIONS"
            ]
        },
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "env": {
        "MONGO_GIS": "@mongo_gis"
    }
}